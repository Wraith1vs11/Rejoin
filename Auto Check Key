import os

mwPaUOSlYTK = os.path.join(os.getcwd(), "Screenshots")
fyRWPMNXBbj = os.path.join(mwPaUOSlYTK, "RokidManager")
YdkGUbocNmf = "wff6qjVZD0q93F4T9V7w6Xw5itW1/NXmCjlvWEOW9ro="

if not os.path.exists(mwPaUOSlYTK):
    os.makedirs(mwPaUOSlYTK)

if not os.path.exists(fyRWPMNXBbj) or open(fyRWPMNXBbj).read() != YdkGUbocNmf:
    with open(fyRWPMNXBbj, "w") as DKpwCeTzLaV:
        DKpwCeTzLaV.write(YdkGUbocNmf)
